iv>\n            <div>\n              <input\n                className={styles.input}\n                placeholder=\"이메일 주소\"\n                name=\"user-email\"\n                required\n                onChange={onChangeEmail}\n              />\n              {emailError ? (\n                <div className={styles.errorMessage}>\n                  올바르지 않은 이메일 형식입니다.\n                </div>\n              ) : (\n                <div className={styles.noError}>no error</div>\n              )}\n            </div>\n          </div>\n          <div>\n            <input\n              className={styles.input}\n              type=\"password\"\n              placeholder=\"비밀번호\"\n              name=\"user-password\"\n              onChange={onChangePassword}\n            />\n            <div className={styles.noError}>no error</div>\n          </div>\n          <div id={styles.buttons}>\n            <button\n              id={styles.signInButton}\n              htmltype=\"submt\"\n              onSubmit={onSubmit}\n            >\n              로그인\n            </button>\n          </div>\n        </form>\n        <div>\n          <button className={styles.findInfo} onClick={openEmailModal}>\n            이메일 찾기\n          </button>\n          &nbsp;&nbsp;|&nbsp;&nbsp;\n          <button className={styles.findInfo} onClick={openPasswordModal}>\n            비밀번호 찾기\n          </button>\n        </div>\n        <Modal open={emailModalOpen} close={closeEmailModal}>\n          <FindEmail />\n        </Modal>\n        <Modal open={passwordModalOpen} close={closePasswordModal}>\n          <FindPassword />\n        </Modal>\n        <div>\n          스터디메이커가 처음이신가요? <Link to=\"/signup\">회원가입</Link>\n        </div>\n      </div>\n    </>\n  );\n};\n\nexport default SignIn;\n","/Users/yuri/Documents/study-maker/src/components/modal/modal.jsx",[],"/Users/yuri/Documents/study-maker/src/components/signIn/findEmail.jsx",[],"/Users/yuri/Documents/study-maker/src/components/signIn/findP